{
  "name": "new-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "-------- build, dev ---------------------------------------------------------------": "-",
    "clean:dev": "rimraf .next && yarn dev",
    "dev": "npx concurrently \"yarn proxy:https\" \"next dev -p 3001\" \"npx wait-on https-get://localhost:3000 -i 1000\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "new:component": "cd .hygen && hygen new component",
    "-------- linting ------------------------------------------------------------------": "-",
    "prettier": "prettier --write",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "lint-staged": "lint-staged",
    "check-types": "tsc --noemit",
    "-------- ssl ---------------------------------------------------------------------": "-",
    "create:https": "mkcert create-ca && mkcert create-cert",
    "proxy:https": "npx local-ssl-proxy --cert ./cert.crt --key ./cert.key --source 3000 --target 3001",
    "-------- schema management--------------------------------------------------------": "-",
    "sb-mig": "sb-mig",
    "components:discover": "sb-mig discover components --all --write --file=../src/components/all-components",
    "init:project": "sb-mig init project",
    "postinit:project": "yarn sync:components:all && yarn sync:contents:all && yarn create:https",
    "sync:components:all": "sb-mig sync components --all --presets",
    "sync:datasources:all": "sb-mig sync datasources --all",
    "sync:roles:all": "sb-mig sync roles --all",
    "sync:content:seed": "sb-mig sync content --syncDirection fromFileToSpace --stories --from workshop-seed --yes",
    "sync:all": "yarn sync:roles:all && yarn sync:datasources:all && yarn sync:components:all && yarn sync:contents:all",
    "prepare": "husky install"
  },
  "dependencies": {
    "@axe-core/react": "^4.5.2",
    "@ef-global/backpack": "^1.5.0",
    "@storyblok/react": "^2.0.15",
    "next": "^13.1.4",
    "next-seo": "^5.15.0",
    "next-sitemap": "^4.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storyblok-js-client": "^5.8.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.4",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.1.4",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "lint-staged": "^13.1.1",
    "local-ssl-proxy": "^1.3.0",
    "mkcert": "^1.5.1",
    "prettier": "^2.8.6",
    "rimraf": "^4.4.0",
    "sass": "^1.59.3",
    "sb-mig": "^5.0.8-beta.3",
    "storyblok-schema-types": "^0.4.33",
    "tinycolor2": "^1.6.0",
    "tsc-files": "^1.1.3",
    "typescript": "^4.9.5"
  }
}
